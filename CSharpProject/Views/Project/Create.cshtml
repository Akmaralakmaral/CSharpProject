@model Domain.Models.Entities.Project

@{
    ViewData["Title"] = "Create Project";
}

<h1>Create Project</h1>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="ProjectName"></label>
        <input asp-for="ProjectName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="StartDate"></label>
        <input asp-for="StartDate" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="EndDate"></label>
        <input asp-for="EndDate" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Priority"></label>
        <input asp-for="Priority" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="CustomerCompanyId"></label>
        <input asp-for="CustomerCompanyId" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="ExecutorCompanyId"></label>
        <input asp-for="ExecutorCompanyId" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="ProjectManagerId"></label>
        <input asp-for="ProjectManagerId" class="form-control" />
    </div>
    

   <button type="submit" class="btn btn-primary">Create</button>
</form>